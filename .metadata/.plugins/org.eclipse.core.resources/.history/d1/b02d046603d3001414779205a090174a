
public class New_Points implements FrequentPoints {
	private int DAYS_NEW_RELEASE = 1;
	
	@Override
	public int points(int daysRented) {
		int points = 0;
		// add frequent renter points
		points++;
		
		// add bonus for a two day new release rental
		    if(daysRented > (DAYS_NEW_RELEASE)) {
		        points ++;;
		}
		return points;
	}

}
