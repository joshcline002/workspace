import java.util.ArrayList;


public class CompositeItem implements Item {
	
	private ArrayList<Item> children = new ArrayList<Item>();
	
	
	@Override
	public void getTitle() {
		for(Item child : children){
			child.getTitle();
		}
	}
	
	public void add(Item item){
		item.setParent(this);
		children.add(item);
	}

}
