import java.util.ArrayList;


public class InsertionSort<E extends Comparable<? super E>> implements  SortAnalysis<E> {

	@Override
	public long analyzeSort(ArrayList<E> list) {
		Comparable newComp;
		int i, j;
		Object newValue = new Object();
		for(i = 1; i< list.size(); i++){
			newComp = (Comparable) list.get(i);
			j = i;
			
			while(j > 0 && -1 == newComp.compareTo((Comparable)list.get(j-i))){
				list.set(j, list.get(j-i));
				j--;
			}
			list.set(j, list.get(i));
		}
		return 0;
	}


}
